<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".doctor.MedicalOfficer">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:src="@drawable/singin_top"
        app:layout_constraintTop_toTopOf="parent"/>


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:src="#0099CC"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="150dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/baseline_sort_24" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginStart="52dp"
        android:layout_marginTop="150dp"
        android:text="SORT BY"
        android:textColor="#2971C4"
        android:textSize="15sp"
        android:textStyle="bold"
        android:textAlignment="center"/>
    <ImageView
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="107dp"
        android:layout_marginTop="142dp"
        app:srcCompat="@drawable/baseline_info1_24"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="60dp"
        android:text="MORE FILTERS"
        android:textAlignment="center"
        android:textColor="#2971C4"
        android:textSize="15sp"
        android:textStyle="bold" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="135dp"
        android:layout_marginBottom="18dp"
        app:srcCompat="@drawable/baseline_sort_24" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="142dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="18dp"
        app:srcCompat="@drawable/baseline_info1_24" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:gravity="left"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:layout_marginStart="20dp"
        android:text="Medical Officer"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:fontFamily="sans-serif"
        tools:targetApi="s" />

    <ImageView
        android:id="@+id/Notifications"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_marginTop="30dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="46dp"
        app:srcCompat="@drawable/baseline_notifications_24" />

    <ImageView
        android:id="@+id/Messages"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_marginTop="30dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="10dp"
        app:srcCompat="@drawable/baseline_message_24"/>

    <SearchView
        android:id="@+id/idSV"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="80dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:iconifiedByDefault="false"
        android:padding="4dp"
        android:queryHint="Search Articles and News"
        android:textSize="11sp"
        android:textColor="@color/black"
        android:background="@drawable/search" />


    <RelativeLayout
        android:id="@+id/btn1"
        android:layout_width="160dp"
        android:layout_height="170dp"
        android:layout_marginTop="200dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/btn"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <ImageView
            android:id="@+id/IV1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="15dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            app:srcCompat="@drawable/baseline_sensors_24"/>

        <TextView
            android:id="@+id/TV1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="85dp"
            android:layout_centerHorizontal="true"
            android:text="Appointments"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textAlignment="center"
            tools:targetApi="s" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/btn3"
        android:layout_width="160dp"
        android:layout_height="170dp"
        android:layout_marginTop="200dp"
        android:background="@drawable/btn"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="10dp"
        android:layout_alignParentTop="true">

        <ImageView
            android:id="@+id/IV3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="15dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            app:srcCompat="@drawable/baseline_groups_24"/>

        <TextView
            android:id="@+id/TV3"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="85dp"
            android:textAlignment="center"
            android:layout_centerHorizontal="true"
            android:text="Waiting Room"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:targetApi="s" />
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/btn4"
        android:layout_width="160dp"
        android:layout_height="170dp"
        android:layout_marginTop="400dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/btn"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <ImageView
            android:id="@+id/IV4"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="15dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            app:srcCompat="@drawable/baseline_vpn_key_24"/>

        <TextView
            android:id="@+id/TV4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="85dp"
            android:layout_centerHorizontal="true"
            android:text="Symptom Checker"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textAlignment="center"
            tools:targetApi="s" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/btn6"
        android:layout_width="160dp"
        android:layout_height="170dp"
        android:layout_marginTop="400dp"
        android:background="@drawable/btn"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="10dp"
        android:layout_alignParentTop="true">

        <ImageView
            android:id="@+id/IV2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="15dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            app:srcCompat="@drawable/baseline_info_24"/>

        <TextView
            android:id="@+id/TV2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="85dp"
            android:textAlignment="center"
            android:layout_centerHorizontal="true"
            android:text="Physical Appointments"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:targetApi="s" />
    </RelativeLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/addContinue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#0099CC"
        android:contextClickable="true"
        android:clickable="true"
        android:layout_marginBottom="85dp"
        android:layout_marginEnd="15dp"
        android:contentDescription="TODO"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        app:srcCompat="@drawable/baseline_add_24"
        android:focusable="true" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:layout_alignParentBottom="true"
        android:background="#0099CC"
        app:menu="@menu/bottom_nav">

    </com.google.android.material.bottomnavigation.BottomNavigationView>




</RelativeLayout>